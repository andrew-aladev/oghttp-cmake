<?xml version="1.0" encoding="utf-8"?>
<root>
  <group mode="include">
    <range>
      <!-- any byte -->
      <from>0x00</from>
      <to>0xFF</to>
    </range>
  </group>
  <group mode="exclude">
    <!-- horizontal tabulation -->
    <byte>0x09</byte>
    <!-- line feed -->
    <byte>0x0A</byte>
    <!-- vertical tabulation -->
    <byte>0x0B</byte>
    <!-- form feed -->
    <byte>0x0C</byte>
    <!-- carriage return -->
    <byte>0x0D</byte>
    <!-- space -->
    <byte>0x20</byte>
  </group>
  <min-length>1</min-length>
  <max-length>0xFFFF</max-length>
</root>
