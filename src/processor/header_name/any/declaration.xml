<?xml version="1.0" encoding="utf-8"?>
<root>
  <!-- Any byte except separator and terminators. -->

  <group mode="include">
    <range>
      <from>0x00</from>
      <to>0xFF</to>
    </range>
  </group>
  <group mode="exclude">
    <!-- separator -->
    <byte>:</byte>
    <!-- line feed (\n) -->
    <byte>0x0A</byte>
    <!-- carriage return (\r) -->
    <byte>0x0D</byte>
  </group>
  <min-length>1</min-length>
  <max-length>0xFFFF</max-length>
</root>
